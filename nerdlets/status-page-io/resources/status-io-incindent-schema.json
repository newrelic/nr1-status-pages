{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
      "page",
      "components",
      "incidents",
      "scheduled_maintenances",
      "status"
    ],
    "properties": {
      "page": {
        "$id": "#/properties/page",
        "type": "object",
        "title": "The Page Schema",
        "required": [
          "id",
          "name",
          "url",
          "time_zone",
          "updated_at"
        ],
        "properties": {
          "id": {
            "$id": "#/properties/page/properties/id",
            "type": "string",
            "title": "The Id Schema",
            "default": "",
            "examples": [
              "4qjjcrpdj8jh"
            ],
            "pattern": "^(.*)$"
          },
          "name": {
            "$id": "#/properties/page/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": [
              "New Relic"
            ],
            "pattern": "^(.*)$"
          },
          "url": {
            "$id": "#/properties/page/properties/url",
            "type": "string",
            "title": "The Url Schema",
            "default": "",
            "examples": [
              "https://status.newrelic.com"
            ],
            "pattern": "^(.*)$"
          },
          "time_zone": {
            "$id": "#/properties/page/properties/time_zone",
            "type": "string",
            "title": "The Time_zone Schema",
            "default": "",
            "examples": [
              "Etc/UTC"
            ],
            "pattern": "^(.*)$"
          },
          "updated_at": {
            "$id": "#/properties/page/properties/updated_at",
            "type": "string",
            "title": "The Updated_at Schema",
            "default": "",
            "examples": [
              "2019-08-27T01:44:39.119Z"
            ],
            "pattern": "^(.*)$"
          }
        }
      },
      "components": {
        "$id": "#/properties/components",
        "type": "array",
        "title": "The Components Schema",
        "items": {
          "$id": "#/properties/components/items",
          "type": "object",
          "title": "The Items Schema",
          "required": [
            "id",
            "name",
            "status",
            "created_at",
            "updated_at",
            "position",
            "description",
            "showcase",
            "group_id",
            "page_id",
            "group",
            "only_show_if_degraded"
          ],
          "properties": {
            "id": {
              "$id": "#/properties/components/items/properties/id",
              "type": "string",
              "title": "The Id Schema",
              "default": "",
              "examples": [
                "y33q160k1wzt"
              ],
              "pattern": "^(.*)$"
            },
            "name": {
              "$id": "#/properties/components/items/properties/name",
              "type": "string",
              "title": "The Name Schema",
              "default": "",
              "examples": [
                "APM"
              ],
              "pattern": "^(.*)$"
            },
            "status": {
              "$id": "#/properties/components/items/properties/status",
              "type": "string",
              "title": "The Status Schema",
              "default": "",
              "examples": [
                "operational"
              ],
              "pattern": "^(.*)$"
            },
            "created_at": {
              "$id": "#/properties/components/items/properties/created_at",
              "type": "string",
              "title": "The Created_at Schema",
              "default": "",
              "examples": [
                "2013-05-10T15:33:41.434Z"
              ],
              "pattern": "^(.*)$"
            },
            "updated_at": {
              "$id": "#/properties/components/items/properties/updated_at",
              "type": "string",
              "title": "The Updated_at Schema",
              "default": "",
              "examples": [
                "2019-08-27T01:25:10.631Z"
              ],
              "pattern": "^(.*)$"
            },
            "position": {
              "$id": "#/properties/components/items/properties/position",
              "type": "integer",
              "title": "The Position Schema",
              "default": 0,
              "examples": [
                1
              ]
            },
            "description": {
              "$id": "#/properties/components/items/properties/description",
              "type": "null",
              "title": "The Description Schema",
              "default": null,
              "examples": [
                null
              ]
            },
            "showcase": {
              "$id": "#/properties/components/items/properties/showcase",
              "type": "boolean",
              "title": "The Showcase Schema",
              "default": false,
              "examples": [
                false
              ]
            },
            "group_id": {
              "$id": "#/properties/components/items/properties/group_id",
              "type": "null",
              "title": "The Group_id Schema",
              "default": null,
              "examples": [
                null
              ]
            },
            "page_id": {
              "$id": "#/properties/components/items/properties/page_id",
              "type": "string",
              "title": "The Page_id Schema",
              "default": "",
              "examples": [
                "4qjjcrpdj8jh"
              ],
              "pattern": "^(.*)$"
            },
            "group": {
              "$id": "#/properties/components/items/properties/group",
              "type": "boolean",
              "title": "The Group Schema",
              "default": false,
              "examples": [
                false
              ]
            },
            "only_show_if_degraded": {
              "$id": "#/properties/components/items/properties/only_show_if_degraded",
              "type": "boolean",
              "title": "The Only_show_if_degraded Schema",
              "default": false,
              "examples": [
                false
              ]
            }
          }
        }
      },
      "incidents": {
        "$id": "#/properties/incidents",
        "type": "array",
        "title": "The Incidents Schema"
      },
      "scheduled_maintenances": {
        "$id": "#/properties/scheduled_maintenances",
        "type": "array",
        "title": "The Scheduled_maintenances Schema"
      },
      "status": {
        "$id": "#/properties/status",
        "type": "object",
        "title": "The Status Schema",
        "required": [
          "indicator",
          "description"
        ],
        "properties": {
          "indicator": {
            "$id": "#/properties/status/properties/indicator",
            "type": "string",
            "title": "The Indicator Schema",
            "default": "",
            "examples": [
              "none"
            ],
            "pattern": "^(.*)$"
          },
          "description": {
            "$id": "#/properties/status/properties/description",
            "type": "string",
            "title": "The Description Schema",
            "default": "",
            "examples": [
              "All Systems Operational"
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    }
  }